    "koa-static": "^4.0.2",
    "koa-static-cache": "^5.1.2",
    "lodash": "^4.17.5",
    "lru-cache": "^4.1.1",
    "mkdirp": "^0.5.1",
    "path-to-regexp": "^2.1.0",
    "qs": "^6.5.1",
    "regenerator-runtime": "^0.11.1",
