{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "*": [
        "*",
        "*.vue"
      ]
    },
    "sourceMap": true,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "es5",
    "typeRoots": [
      "./types",
      "../node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
