{
  "name": "rubick",
  "version": "0.0.1",
  "description": "Alauda front end project Rubick built with Vue stack + TypeScript, just for study.",
  "main": "index.js",
  "repository": "https://github.com/JounQin/Rubick.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "nodemon --exec \"env-cmd .env env-cmd --fallback .env.local ts-node server/router/dev\"",
    "dev": "concurrently \"env-cmd .env env-cmd --fallback .env.local ts-node server\" \"npm run server\"",
    "start": "env-cmd .env ts-node server",
    "build:client": "env-cmd .env webpack --config build/webpack/client.ts --color --progress",
    "build:server": "env-cmd .env webpack --config build/webpack/server.ts --color --progress",
    "build": "rimraf dist && yarn build:client && yarn build:server",
    "lint": "tslint -p . -e 'node_modules/**' -t stylish '**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged"
  },
  "browserslist": ["> 1% in CN"],
  "lint-staged": {
    "*.{json,md}": ["prettier --write", "git add"],
    "*.ts": ["tslint -p . -t stylish --fix", "git add"]
  },
  "nodemonConfig": {
    "ext": "ts",
    "watch": [".env", ".env.local", "server/router"]
  },
  "postcss": {
    "autoprefixer": null
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 120
        }
      }
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "koa": "^2.4.1",
    "koa-better-http-proxy": "^0.2.4",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "koa-session": "^5.5.1",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "qs": "^6.5.1",
    "regenerator-runtime": "^0.11.0",
    "vue": "^2.5.9",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.0",
    "vue-server-renderer": "^2.5.9",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.3",
    "@types/debug": "^0.0.30",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/friendly-errors-webpack-plugin": "^0.1.1",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/koa": "^2.0.42",
    "@types/koa-bodyparser": "^3.0.26",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-compress": "^2.0.8",
    "@types/koa-logger": "^2.0.3",
    "@types/koa-router": "^7.0.27",
    "@types/koa-session": "^3.0.6",
    "@types/koa-static": "^3.0.2",
    "@types/lodash": "^4.14.87",
    "@types/lru-cache": "^4.1.0",
    "@types/memory-fs": "^0.3.0",
    "@types/node": "^8.0.54",
    "@types/pug": "^2.0.4",
    "@types/qs": "^6.5.1",
    "@types/webpack": "^3.8.1",
    "@types/webpack-dev-middleware": "^1.9.4",
    "@types/webpack-hot-middleware": "^2.16.2",
    "@types/webpack-merge": "^4.1.1",
    "@types/webpack-node-externals": "^1.6.0",
    "autoprefixer": "^7.2.1",
    "chokidar": "^1.7.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "debug": "^3.1.0",
    "env-cmd": "^7.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "fonts.css": "^1.5.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.8.2",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "resolve-url-loader": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.9",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
