{
  "name": "rubick",
  "version": "0.0.1",
  "description": "Alauda front end project Rubick built with Vue stack + TypeScript, just for study.",
  "repository": "https://github.com/JounQin/Rubick.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server:core": "env-cmd .env env-cmd --fallback .env.local ts-node -r tsconfig-paths/register server",
    "server:router": "env-cmd .env env-cmd --fallback .env.local ts-node -r tsconfig-paths/register server/router/dev",
    "watch": "nodemon --exec \"npm run server:router\"",
    "dll:check": "ts-node build/dll-check.ts",
    "dll:cache": "mkdir -p .cache && cp dist/vendors.dll.manifest.json .cache && cp dist/static/vendors.dll.*.js .cache",
    "dll:restore": "bash build/dll-restore.sh",
    "dev": "env-cmd .env env-cmd --fallback .env.local yarn dll:check && run-p server:core watch",
    "start": "env-cmd --fallback .env.local env-cmd .env node dist/server",
    "build:dll": "webpack --config build/dll.ts --color --progress",
    "build:server": "webpack --config build/server.ts --color --progress",
    "build:template": "mkdir -p dist/static && ts-node build/template",
    "build:vue:client": "webpack --config build/vue-client.ts --color --progress",
    "build:vue:server": "webpack --config build/vue-server.ts --color --progress",
    "build": "bash build/build.sh",
    "deploy": "bash build/deploy.sh",
    "sync:pull": "bash build/sync-pull.sh",
    "sync:push": "bash build/sync-push.sh",
    "lint": "tslint -p . -t stylish '**/*.ts'",
    "precommit": "lint-staged",
    "pm2": "pm2 start --name=rubick npm -- start",
    "prune": "yarn install --production --ignore-optional --ignore-scripts --prefer-offline --force"
  },
  "browserslist": ["> 1% in CN"],
  "lint-staged": {
    "*.{json,md,vue}": ["prettier --write", "git add"],
    "*.ts": ["tslint -p . -t stylish --fix", "git add"]
  },
  "nodemonConfig": {
    "ext": "ts",
    "watch": [".env", ".env.local", "server/router"]
  },
  "postcss": {
    "autoprefixer": null
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 150
        }
      }
    ]
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "axios": "^0.18.0",
    "captchapng2": "^1.0.3",
    "date-fns": "^2.0.0-alpha.7",
    "debug": "^3.1.0",
    "env-cmd": "^7.0.0",
    "koa": "^2.5.0",
    "koa-better-http-proxy": "^0.2.4",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.1",
    "koa-static": "^4.0.2",
    "koa-static-cache": "^5.1.2",
    "lodash": "^4.17.5",
    "lru-cache": "^4.1.2",
    "mkdirp": "^0.5.1",
    "path-to-regexp": "^2.2.0",
    "pug-html-loader": "^1.1.5",
    "qs": "^6.5.1",
    "regenerator-runtime": "^0.11.1",
    "v-tooltip": "^2.0.0-rc.31",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vue-translator": "^0.7.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/extract-text-webpack-plugin": "^3.0.2",
    "@types/friendly-errors-webpack-plugin": "^0.1.1",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/koa": "^2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-compress": "^2.0.8",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-router": "^7.0.27",
    "@types/koa-session": "^5.7.1",
    "@types/koa-static": "^4.0.0",
    "@types/koa-webpack": "^1.0.0",
    "@types/lodash": "^4.14.105",
    "@types/lru-cache": "^4.1.0",
    "@types/memory-fs": "^0.3.0",
    "@types/mkdirp": "^0.5.2",
